{
  "icon_theme": "Material Icon Theme",
  "features": {
    "edit_prediction_provider": "zed"
  },
  "theme": "Tokyo Night",
  "agent": {
    "default_profile": "write",
    "default_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-latest"
    }
  },
  "base_keymap": "VSCode",
  "ui_font_family": "JetBrains Mono",
  "buffer_font_family": "JetBrains Mono",
  "ui_font_size": 14,
  "code_actions_on_format": {
    "source.fixAll.eslint": true
  },
  "autosave": "off",
  "soft_wrap": "editor_width",
  "terminal": {
    "font_size": 14,
    "font_family": "JetBrainsMono Nerd Font",
    "copy_on_select": true
  },
  "ui_font_features": {
    "calt": true
  },
  "tab_size": 2,
  "preferred_line_length": 100,
  "buffer_font_size": 14,
  "format_on_save": "on",
  "formatter": {
    "code_actions": {
      "source.fixAll.eslint": true
    }
  },
  "languages": {
    "HTML": {
      "language_servers": [
        "vscode-html-language-server",
        "superhtml"
      ],
      "formatter": {
        "language_server": {
          "name": "superhtml"
        }
      }
    },
    "Go": {
      "format_on_save": "on",
      "formatter": "language_server"
    },
    "JavaScript": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "code_actions_on_format": {
        "source.fixAll.biome": true,
        "source.organizeImports.biome": true
      }
    },
    "TypeScript": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "code_actions_on_format": {
        "source.fixAll.biome": true,
        "source.organizeImports.biome": true
      }
    },
    "TSX": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "code_actions_on_format": {
        "source.fixAll.biome": true,
        "source.organizeImports.biome": true
      }
    },
    "JSON": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "code_actions_on_format": {
        "source.fixAll.biome": true,
        "source.organizeImports.biome": true
      }
    },
    "JSONC": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "code_actions_on_format": {
        "source.fixAll.biome": true,
        "source.organizeImports.biome": true
      }
    },
    "CSS": {
      "formatter": {
        "language_server": {
          "name": "biome"
        }
      },
      "code_actions_on_format": {
        "source.fixAll.biome": true,
        "source.organizeImports.biome": true
      }
    }
  }
}
