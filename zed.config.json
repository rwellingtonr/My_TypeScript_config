{
  "icon_theme": "Material Icon Theme",
  "features": {
    "edit_prediction_provider": "zed"
  },
  "theme": "Visual Assist Dark",
  "agent": {
    "default_profile": "write",
    "version": "2",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    }
  },
  "base_keymap": "VSCode",
  "ui_font_family": "JetBrains Mono",
  "buffer_font_family": "JetBrains Mono",
  "ui_font_size": 16,
  "code_actions_on_format": {
    "source.fixAll.eslint": true
  },
  "autosave": "off",
  "soft_wrap": "editor_width",
  "hour_format": "hour24",
  "terminal": {
    "font_size": 14,
    "font_family": "JetBrainsMono Nerd Font",
    "copy_on_select": true
  },
  "ui_font_features": {
    "calt": true
  },
  "tab_size": 2,
  "preferred_line_length": 100,
  "buffer_font_size": 16,
  "format_on_save": "language_server",
  "formatter": {
    "code_actions": {
      "source.fixAll.eslint": true
    }
  },
  "languages": {
    "JSON": {
      "format_on_save": "on",
      "formatter": "language_server"
    },
    "HTML": {
      "language_servers": [
        "vscode-html-language-server",
        "superhtml"
      ],
      "formatter": {
        "language_server": {
          "name": "superhtml"
        }
      }
    },
    "CSS": {
      "format_on_save": "on",
      "formatter": "auto"
    },
    "Go": {
      "format_on_save": "on",
      "formatter": "language_server"
    }
  }
}
